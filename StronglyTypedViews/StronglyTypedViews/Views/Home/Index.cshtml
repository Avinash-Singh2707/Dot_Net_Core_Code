@* @model StronglyTypedViews.Models.Employee *@        @* You can full path of that model  *@
@* @model Employee *@   @* Now binding is successfull, NOW it becomes strongly typed view of Employee model class. *@

@* @model List<Employee> *@

@model IEnumerable<Employee>        //you can use IEnumerable and list of employee

@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>Learn about <a href="https://learn.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
</div>

